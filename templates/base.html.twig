                        <span class="navbar-toggler-icon"></span>
                    </button>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link text-primary px-3" href="/default">Nous contacter</a>
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                  </li>
                  <li class="nav-item">
                    <div class="d-grid gap-2 d-md-flex justify-content-md-end">
                    <a class="nav-link text-primary px-3" href="/connexion">Connexion</a>
                    <button class="navbar-toggler btn-sm btn-outline-primary btn btn-primary me-md-2" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    </div>
                  </li>
                </ul>
              </div>
            </div>
          </nav>
